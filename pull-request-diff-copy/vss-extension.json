{
    "manifestVersion": 1,
    "id": "pull-request-diff-copy",
    "name": "Pull Request Diff Copy",
    "version": "0.1.0",
    "publisher": "lean-soft",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Copy a git branch of all changed files to diff directory",
    "categories": [
        "Build and release"
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "task"
        }
    ],
    "content": {
        "details": {
            "path": "readme.md"
        }
    },
	 "tags": [
	    "Copy",
	    "Branch Changes",
		"Git Diff",
	    "Build",
	    "Git"
	  ],
	  	"screenshots": [
	    
	  ],
		"repository": {
			"type":"git",
	      	"uri": "https://github.com/lean-soft/pull-request-diff-copy"
	    },
    "contributions": [
        {
            "id": "pull-request-diff-copy",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task"
            }
        }
    ]
}